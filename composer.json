{
    "name": "edionme/anchortextchecker",
    "description": "This package allows to crawl a website and check anchor text and nofollow",
    "homepage": "https://github.com/edionmelarosa/anchortextchecker",
    "license": "MIT",
    "keywords": [
        "edionme",
        "crawler",
        "seo",
        "nofollow",
        "website"
    ],
    "authors": [
        {
            "name": "Espiridion Jr Larosa",
            "email": "contact@edionme.com"
        }
    ],
    "require": {
        "symfony/dom-crawler": "^5.1",
        "symfony/css-selector": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Edionme\\AnchorTextChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Edionme\\AnchorTextChecker\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
